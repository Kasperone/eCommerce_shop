	"bugs": {
		"url": "https://github.com/Kasperone/eCommerce_shop/issues"
	},
	"homepage": "https://github.com/Kasperone/eCommerce_shop#readme",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"colors": "^1.4.0",
		"dotenv": "^16.0.0",
		"express": "^4.18.1",
		"express-async-handler": "^1.2.0",
		"mongoose": "^6.3.1"
	},
	"devDependencies": {
		"concurrently": "^7.1.0",
		"nodemon": "^2.0.16"
	}
